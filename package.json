{
  "name": "cosey-admin",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --force",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky",
    "eslint": "eslint --cache --max-warnings 0  \"**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue}\" --fix",
    "eslint:check": "eslint --cache --max-warnings 0  \"**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue}\"",
    "prettier": "prettier --write \"**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,css,scss,less,styl,md,html}\"",
    "prettier:check": "prettier --check \"**/*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,css,scss,less,styl,md,html}\"",
    "commit": "commit",
    "new-component": "tsx ./scripts/new-component.ts",
    "nc": "npm run new-component",
    "docs:dev": "cross-env NODE_ENV=development vitepress dev docs --force --port 5174",
    "docs:build": "cross-env NODE_ENV=production vitepress build docs",
    "docs:build-debug": "cross-env NODE_ENV=production DEBUG=true vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "docs:add-cosey": "pnpm -F @cosey/docs add file:../lib-dist/cosey",
    "cosey:version": "tsx ./scripts/version.ts",
    "cosey:build": "tsx ./scripts/build.ts",
    "cosey:publish": "tsx ./scripts/publish.ts",
    "cosey:deploy": "tsx ./scripts/deploy.ts",
    "changelog": "tsx ./scripts/changelog.ts",
    "tag": "tsx ./scripts/tag.ts"
  },
  "lint-staged": {
    "*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue}": "eslint --fix",
    "*.{js,mjs,cjs,jsx,ts,mts,cts,tsx,vue,css,scss,less,styl,md,html}": "prettier --write"
  },
  "dependencies": {
    "@casl/ability": "^6.7.3",
    "@casl/vue": "^2.2.2",
    "@cosey/mock": "workspace:^",
    "@gunny/mock": "^1.2.0",
    "@iconify-json/carbon": "^1.2.8",
    "@tailwindcss/vite": "^4.0.9",
    "axios": "^1.7.9",
    "cosey": "workspace:^",
    "dayjs": "^1.11.13",
    "element-plus": "^2.11.4",
    "lodash-es": "^4.17.21",
    "vue": "^3.5.21",
    "vue-i18n": "^11.1.10",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.27.1",
    "@commitlint/cli": "^19.6.0",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/prompt-cli": "^19.6.0",
    "@eslint/compat": "^1.2.9",
    "@eslint/js": "^9.27.0",
    "@rollup/plugin-json": "^6.1.0",
    "@types/babel__core": "^7.20.5",
    "@types/conventional-changelog": "^6.0.0",
    "@types/crypto-js": "^4.2.2",
    "@types/fs-extra": "^11.0.4",
    "@types/gh-pages": "^6.1.0",
    "@types/identicon.js": "^2.3.5",
    "@types/inquirer": "^9.0.7",
    "@types/lodash-es": "^4.17.12",
    "@types/markdown-it": "^14.1.2",
    "@types/semver": "^7.7.0",
    "@types/shelljs": "^0.8.17",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/babel-plugin-jsx": "^1.4.0",
    "@vue/runtime-core": "^3.5.13",
    "@vue/shared": "^3.5.13",
    "consola": "^3.3.3",
    "conventional-changelog": "^7.1.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.25.4",
    "eslint": "^9.27.0",
    "eslint-plugin-vue": "^10.1.0",
    "fs-extra": "^11.2.0",
    "gh-pages": "^6.3.0",
    "glob": "^11.0.2",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "inquirer": "^12.3.0",
    "lint-staged": "^15.2.10",
    "markdown-it": "^14.1.0",
    "ora": "^8.2.0",
    "prettier": "3.4.2",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "rimraf": "^6.0.1",
    "rollup": "^4.40.2",
    "sass": "^1.82.0",
    "semver": "^7.7.1",
    "shelljs": "^0.10.0",
    "tailwindcss": "^4.0.9",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.32.1",
    "vite": "^7.1.7",
    "vite-plugin-svg-icons": "^2.0.1",
    "vite-plugin-vue-devtools": "^8.0.2",
    "vitepress": "^1.6.3",
    "vue-tsc": "^2.2.8"
  }
}
